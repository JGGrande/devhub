generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["relationJoins", "postgresqlExtensions"]
}

datasource db {
  provider   = "postgresql"
  url        = env("DATABASE_URL")
  extensions = [unaccent]
}

model niveis {
  id    Int    @id @default(autoincrement())
  nivel String @db.VarChar()

  @@map("niveis")
}
